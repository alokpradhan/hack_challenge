<%= form_for search do |f| %>

  <%= f.label :origin %>
  <%= f.text_field :origin %>

  <%= f.label :destination %>
  <%= f.text_field :destination %>

  <%= f.label :agency %>
  <%= f.text_field :agency %>

  <%= f.label :stop %>
  <%= f.text_field :stop %>

  <%= f.submit 'Find!', class: "btn btn-block btn-default" %>

<% end %>